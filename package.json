{
  "name": "bitcoin-sv-minting-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"./**/*.+(js|jsx|mjs|cjs|ts|tsx|json|css|scss|sass|md|html)\"",
    "lint-staged": "lint-staged --concurrent false",
    "pretty-quick": "pretty-quick --staged",
    "prepare": "rm -rf .husky && husky install && husky add .husky/pre-commit \"yarn lint-staged \nyarn pretty-quick\""
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "yarn format",
      "yarn lint:fix",
      "yarn lint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "bsv-wallet": "^1.1.19",
    "flowbite": "^1.5.5",
    "flowbite-react": "^0.3.7",
    "next": "^13.1.1",
    "next-pwa": "^5.6.0",
    "prop-types": "^15.8.1",
    "qrcode.react": "^3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.1",
    "reduxjs-toolkit-persist": "^7.2.1",
    "scryptlib": "^2.0.0-beta.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.2.4"
  }
}
